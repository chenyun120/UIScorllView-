#import "UILabel+NibStyle.h"

@implementation UILabel (NibStyle)

@dynamic nibTextColor;

- (void)setNibTextColor:(NSString *)nibTextColor
{
    self.textColor = [UIColor colorWithHexString:nibTextColor];;
}

@end

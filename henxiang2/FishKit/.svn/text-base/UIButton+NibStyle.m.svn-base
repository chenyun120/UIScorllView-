#import "UIButton+NibStyle.h"

@implementation UIButton (NibStyle)

@dynamic nibTintColor;

- (void)setNibTintColor:(NSString *)tintColor
{
    [self setTitleColor:[UIColor colorWithHexString:tintColor] forState:UIControlStateNormal];
}

@end
